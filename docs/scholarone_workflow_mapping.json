  "workflows": {
    "basic_submission_report": {
      "description": "Basic manuscript information with authors",
      "required_columns": [
        "Journal Name",
        "Submission ID",
        "Title",
        "Status",
        "Author Name",
        "Submission Date"
      ],
      "workflow_steps": [
        {
          "step": 1,
          "endpoint": "4",
          "name": "Get IDs By Date",
          "purpose": "Get list of submissions in date range",
          "params": [
            "from_time",
            "to_time",
            "site_name"
          ],
          "provides": [
            "submissionId",
            "documentId",
            "documentStatusName",
            "datetimeCreated",
            "submittingAuthorId"
          ]
        },
        {
          "step": 2,
          "endpoint": "1",
          "name": "Get Submission Info Basic",
          "purpose": "Get submission titles and basic info",
          "params": [
            "ids",
            "site_name"
          ],
          "input_from_step": 1,
          "input_field": "submissionId",
          "provides": [
            "submissionTitle",
            "submissionStatus",
            "authorFullName",
            "submissionDate"
          ]
        }
      ],
      "field_mappings": {
        "Journal Name": "static:Organization Science",
        "Submission ID": "step1:submissionId",
        "Title": "step2:submissionTitle",
        "Status": "step1:documentStatusName",
        "Author Name": "step2:authorFullName",
        "Submission Date": "step2:submissionDate"
      }
    },
